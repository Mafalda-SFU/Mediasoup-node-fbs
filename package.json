{
  "name": "@mafalda-sfu/mediasoup-node-tests",
  "version": "3.12.11",
  "description": "Node.js tests extracted from Mediasoup",
  "exports": {
    ".": "./index.js",
    "./*": "./lib/test-*.js"
  },
  "scripts": {
    "build": "tsc 1> /dev/null; cp --recursive src/data/ lib/",
    "prepare": "npm run build",
    "release": "scripts/release.js",
    "test": "jest",
    "update": "scripts/update.js"
  },
  "keywords": [
    "Mediasoup",
    "Mafalda",
    "SFU",
    "tests"
  ],
  "author": "Jesús Leganés-Combarro 'piranna' (https://piranna.github.io)",
  "license": "ISC",
  "dependencies": {
    "mediasoup": "^3.12.11",
    "pick-port": "^1.0.1",
    "sctp": "^1.0.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@npmcli/package-json": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "simple-git": "^3.22.0",
    "tar-stream": "^3.1.6",
    "typescript": "^5.3.3"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mafalda-SFU/Mediasoup-node-tests.git"
  },
